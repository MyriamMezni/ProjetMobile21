<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityHotelFrance">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="cursive"
        android:text="Hebergement"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="50dp" />

    <Button
        android:id="@+id/RetourFranceBT"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Icon"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal|center"
        android:layout_marginStart="94dp"
        android:layout_marginTop="544dp"
        android:text="Retour"
        tools:ignore="DuplicateIds" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/navigation"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@android:color/white"
        app:menu="@menu/bottom_menu"/>

    <TextView
        android:id="@+id/nomHotel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="624dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/adresse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="624dp"
        android:layout_toEndOf="@+id/nomHotel"
        android:text="TextView" />

    <TextView
        android:id="@+id/pension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="14dp"
        android:layout_marginBottom="623dp"
        android:layout_toEndOf="@+id/adresse"
        android:text="TextView" />

    <TextView
        android:id="@+id/nbEtoile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="622dp"
        android:layout_toEndOf="@+id/pension"
        android:text="TextView" />

    <TextView
        android:id="@+id/prix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginBottom="623dp"
        android:layout_toEndOf="@+id/nbEtoile"
        android:text="TextView" />

    <TextView
        android:id="@+id/service1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="586dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/service2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="586dp"
        android:layout_toEndOf="@+id/service1"
        android:text="TextView" />

    <TextView
        android:id="@+id/service3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="9dp"
        android:layout_marginBottom="584dp"
        android:layout_toEndOf="@+id/service2"
        android:text="TextView" />

    <TextView
        android:id="@+id/equipements"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="14dp"
        android:layout_marginBottom="584dp"
        android:layout_toEndOf="@+id/service3"
        android:text="TextView" />

    <TextView
        android:id="@+id/note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="582dp"
        android:layout_toEndOf="@+id/equipements"
        android:text="TextView" />

</RelativeLayout>